*     CTPTYPE=event

      REAL*4 GFRX_ADC_PED,GFRY_ADC_PED
      REAL*4 GFRX_RAW_ADC,GFRY_RAW_ADC
      REAL*4 GFRX_ADC,GFRY_ADC
      REAL*4 GFRX_SYNC,GFRY_SYNC
      REAL*4 GFRX_SYNC_MEAN,GFRY_SYNC_MEAN
      REAL*4 GFRX,GFRY
      REAL*4 GFRXP,GFRYP
