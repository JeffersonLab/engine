     &     sscat_win_thk,sscat_win_den,sscat_win_a,sscat_win_z,
     &     sdet_ent_thk,sdet_ent_den,sdet_ent_a,sdet_ent_z,
     &     gcell_radius,gz_cell,ga_cell,gcell_den,gwall_thk,gend_thk,
     &     gfront_thk,gair_dens,gair_thk,gair_a,gair_z,
